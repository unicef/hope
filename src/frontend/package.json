{
  "name": "frontend",
  "version": "4.8.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build-for-backend": "bun run build --outDir '../hope/apps/web/static/web' --emptyOutDir",
    "build-and-watch": "bun run build-for-backend --watch",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "test": "vitest",
    "preview": "vite preview",
    "download-dev-openapi": "wget --no-check-certificate -O data/openapi.yml http://localhost:8080/api/rest/",
    "generate-rest-api-types": "bun run generate-rest-api-types-camelcase",
    "generate-rest-api-types-camelcase": "bun run download-dev-openapi && node enhance_and_camelize_openapi.js && bunx openapi-typescript-codegen --input data/openapi-camelcase.yml --output src/restgenerated --useOptions --postfixModels CamelCase --exportCore false",
    "prune-unused-exports": "bunx ts-prune src | grep -v '(used in module)' | grep -v '/index.'"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.28.6",
    "@babel/plugin-transform-typescript": "^7.28.6",
    "@base-ui-components/react": "^1.0.0-rc.0",
    "@date-io/moment": "^3.0.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.3.7",
    "@mui/styled-engine-sc": "^7.3.7",
    "@mui/system": "^7.3.7",
    "@mui/x-date-pickers": "^7.25.0",
    "@mui/x-tree-view": "^8.24.0",
    "@sentry/react": "^10.33.0",
    "@tanstack/react-query": "^5.90.17",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@testing-library/dom": "^10.4.1",
    "@types/lodash": "^4.17.23",
    "@types/node": "^25.0.8",
    "@vitejs/plugin-react": "^5.1.2",
    "camelcase": "^9.0.0",
    "chart.js": "^4.4.8",
    "chartjs-plugin-annotation": "^3.0.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "clsx": "^2.1.0",
    "cross-spawn": "^7.0.3",
    "date-fns": "2.29.3",
    "eslint-import-resolver-alias": "^1.1.2",
    "form-data": "^4.0.4",
    "formik": "^2.4.5",
    "glob": "^13.0.0",
    "helmet": "^8.0.0",
    "i18next": "^25.7.4",
    "jsdom": "^27.4.0",
    "localforage": "^1.10.0",
    "lodash": "^4.17.23",
    "moment": "^2.30.1",
    "path": "^0.12.7",
    "prop-types": "^15.7.0",
    "react": "^19.2.3",
    "react-chartjs-2": "^5.2.0",
    "react-clipboard.js": "^2.0.16",
    "react-dom": "^19.2.3",
    "react-dropzone": "^14.2.3",
    "react-i18next": "^16.5.3",
    "react-idle-timer": "^5.7.2",
    "react-router-dom": "^7.12.0",
    "react-use": "^17.5.0",
    "react-zoom-pan-pinch": "^3.7.0",
    "string.prototype.replaceall": "^1.0.11",
    "styled-components": "^6.3.6",
    "tough-cookie": "^6.0.0",
    "ts-node": "^10.9.2",
    "use-deep-compare-effect": "^1.8.1",
    "uuid": "^13.0.0",
    "vite": "^7.3.1",
